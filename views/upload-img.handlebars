<div class="col-12 col-md-9 col-lg-10 ml-md-auto content"> 
  <h1>Upload Images</h1>
  <div class="upload-form">
    <form method="post" enctype="multipart/form-data" action="/admin/team/wheels/upload-image">
      <div class="form-group">
        <label>Images</label>
        <input type="file" name="img" class="form-control">
      </div>
      <div class="form-group">
        <label>Description</label>
        <textarea type="text" name="description" class="form-control text-area" placeholder="Description"> </textarea>
      </div>
      <button type="submit" class="btn btn-primary">Upload</button>
    </form>
  </div>
</div>